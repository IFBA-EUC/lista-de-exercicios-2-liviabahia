public class contato  {
  private String nome;
  private int num;
  public int contato;
  
  String getNome() {
    return nome;
  }

  void setNome(String nome) {
    this.nome = nome;
  }


  int getNumero() {
    return num;
  }

  void setNumero(int num) {
    this.num = num;
  }

  public void contato(String nome, int num) {
    this.nome = nome;
    this.num = num;
          }
        public void dados() {
          System.out.println("Contato:" +this.getNome()+ " : " +this.getNumero());
        }
        }
        
        
    public class agenda {
      public int c1;
      public int c2;
      public int c3;
      public int c4;

